{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
		"The Panda Oliver"
  ],
	"icon": "assets/pandalib/icon.png",
	"contact": {
		"issues": "https://github.com/PandaDap2006/PandaLib/issues",
		"sources": "https://github.com/PandaDap2006/PandaLib"
	},
  "license": "LGPL-3",
  "environment": "*",
  "entrypoints": {
    "main": [
			"me.pandamods.pandalib.fabric.PandaLibFabric"
    ]
